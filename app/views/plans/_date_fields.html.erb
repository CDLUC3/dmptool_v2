<div class="control-group">
	<%= f.label :creation_date, :class => "control-label" %>
	<div class="controls">
		<%= f.text_field :created_at, value: @plan.created_at.strftime("%m/%d/%Y %I:%M %p"), disabled: true, :class => 'datepicker' %>
	</div>
</div>
<div class="control-group">
	<%= f.label :modification_date, 'Last Modification Date' :class => "control-label" %>
	<div class="controls">
		<%= f.text_field :updated_at, value: @plan.created_at.strftime("%m/%d/%Y %I:%M %p"), disabled: true, :class => 'datepicker' %>
	</div>
</div>