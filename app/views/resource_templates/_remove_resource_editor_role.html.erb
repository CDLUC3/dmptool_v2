<h5> Resource Editors </h5>
<br/>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th> Name </th>
      <th> Username </th>
      <th> Email </th>
      <th> Remove Role </th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.full_name %></td>
      <td><%= user.login_id %></td>
      <td><%= user.email %></td>
      <td><%= link_to raw('<i class="icon-minus-sign icon-white"></i>'), remove_resource_editor_role_path(user_id: user.id), class: "img-polaroid btn-danger" %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<div class="pagination">
  <%= paginate @users %>
</div>