<%- if @comment.errors.any? %>
  console.log('Error');
  $('#comment_dialog_form').html('<%= escape_javascript(render('form')) %>');
<%- else %>
  console.log('Created');
  $('#comment_dialog_form').dialog('close');
	$('.ajax_load_owner').html("<%=raw escape_javascript(render(:partial => "index_owner_comments"))%>");
	$('.ajax_load_reviewer').html("<%=raw escape_javascript(render(:partial => "index_reviewer_comments"))%>");
<%- end %>