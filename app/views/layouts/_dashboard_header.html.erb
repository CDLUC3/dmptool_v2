<div class ='dashboard_header'>
	<%= link_to 'My Dashboard ', dashboard_path %>
	|
	<%= link_to 'My DMPs', '#' %>
	|
	<%= link_to 'Create New DMP' ,'#' %>
	<% if (current_user.has_role?(4) ||  current_user.has_role?(1))%>
		|
		<%= link_to 'Review DMPs', '#' %>
	<%end%>

	<% if (current_user.has_role?(1) ||  current_user.has_role?(5))%>
		|
	<%= link_to 'DMP Templates', requirements_templates_path %>
	<%end%>


	<% if (current_user.has_role?(2) ||  current_user.has_role?(1) ||  current_user.has_role?(5))%>
		|
		<%= link_to 'Resources', resource_templates_path %>
	<%end%>
	
	<% if ( current_user.has_role?(5) ||  current_user.has_role?(1) )%>
		|
		<%= link_to 'Institution Profile', institutions_path %>
	<%end%>
	|
	<%= link_to 'My Profile', edit_user_path(current_user) %>

	<% if current_user.has_role?(1) %>
	|
			
		<%=  link_to 'DMP Administration', users_path %>

	<% end %>
</div>