<h2 class="dashboard-heading"><span class="icon inst-profile"></span>Institution Profile</h2>
<div class="tabbable">
  <ul class="nav nav-tabs">
    <li class="active"><a href="#tab1" data-toggle="tab">Institution Information</a></li>
    <li><a href="#tab3" data-toggle="tab">Institution Hierarchy</a></li>
    <li><a href="#tab2" data-toggle="tab">Institutional Resources</a></li>
    <li><a href="#tab4" data-toggle="tab">Manage Users</a></li>
  </ul>
  <div class="tab-content">

    <div class="tab-pane active" id="tab1">
      <%= render 'form' %>
    </div>

     <div class="tab-pane" id="tab2">
        <%= render 'institutional_resources' %>
    </div>

    <div class="tab-pane " id="tab3">
    	<div class="institutions">
	      <%= nested_institutions @institutions.arrange(order: :created_at) %>
	    </div>
	    <div class="buttons">
	     <%= link_to 'Add New Institution', new_institution_path(5), :class => "btn btn-green" %>
	    </div>
    </div>

    <div class="tab-pane" id="tab4">
        <%= render 'manage_users' %>
    </div>

  </div>
</div>

<script type="text/javascript">
  tab_jump();
  add_tab_to_pagination();
</script>



 
    
  

