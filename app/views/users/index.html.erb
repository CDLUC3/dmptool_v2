<h2 class="dashboard-heading"><span class="icon admin"></span>DMP Administration</h2>
<div class="tabbable">
  <ul class="nav nav-tabs">
    <li class="active"><a href="#tab1" data-toggle="tab">Users</a></li>
    <li><a href="#tab2" data-toggle="tab">Institutions</a></li>
  </ul>
  <div class="tab-content">
    
    <div class="tab-pane table active" id="tab1">
      <%= render 'user_search' %>
      
      <%if params[:q].to_s != '' %>
        <%= render 'users_list' %>
      <% end %>
    
    </div>
    
    <div class="tab-pane table " id="tab2">
      <%= render 'institutions_list' %>
    </div>
  
  </div>
</div>

<script type="text/javascript">
	tab_jump();
	add_tab_to_pagination();
</script>

